{
  "name": "start-imba",
  "version": "0.3.0",
  "description": "Project generator for imba",
  "license": "MIT",
  "bin": "bin/start-imba",
  "engines": {
    "node": ">=8",
    "npm": ">=6.8.0"
  },
  "bugs": "https://github.com/athif23/start-imba/issues",
  "scripts": {
    "format": "prettier --write **/*.{js,json} && standard --fix",
    "build-bin": "pkg . --out-path bin",
    "lint": "standard",
    "test": "jest --runInBand",
    "watch": "jest --watch",
    "snapupdate": "jest --updateSnapshot",
    "coverage": "jest --coverage"
  },
  "files": [
    "src",
    "LICENSE",
    "README.md",
    "docs",
    "bin"
  ],
  "keywords": [
    "cli-app",
    "cli",
    "imba",
    "parcel",
    "webpack"
  ],
  "dependencies": {
    "enquirer": "^2.3.2",
    "execa": "^1.0.0",
    "gluegun": "^5.0.0",
    "latest-version": "^6.0.0",
    "update-notifier": "^6.0.0"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "mock-stdin": "^0.3.1",
    "prettier": "^1.12.1",
    "standard": "^12.0.1",
    "tempy": "^0.3.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
